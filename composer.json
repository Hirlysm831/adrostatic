{
    "name": "adrorocker/adrostatic",
    "description": "A simple Markdown based Blog",
    "license": "MIT",
    "keywords": ["md", "php", "markdown", "html", "adrorocker"],
    "homepage": "https://github.com/adrorocker/adrostatic",
    "type": "library",
    "authors": [
        {
            "name": "Adro Rocker",
            "email": "alejandro.morelos@jarwebdev.com",
            "homepage": "https://github.com/adrorocker"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "pimple/pimple": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "AdroStatic\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AdroStatic\\Tests\\": "tests/"
        },
        "files": [
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
